<h1>Edit your profile</h1>
<p>Use this form...</p>

<%= form_for @user, url: update_user_path do |f|  %>

<%= f.label :first_name %>
<%= f.text_field :first_name %>

<%= f.label :last_name %>
<%= f.text_field :last_name %>

<%= f.label :current_city %>
<%= f.text_field :current_city %>

<%= f.submit "Update User" %>

<% end %>